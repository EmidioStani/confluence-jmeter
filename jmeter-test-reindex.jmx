finished = props.get(&quot;index.finished&quot;);
time = 0;
counter =0;
failed = 0;

while (finished != &quot;true&quot;)
{
  if (counter &gt;= 18000)
  {
    failed = 1;
    break;
  }
  counter = counter + 1;
  java.lang.Thread.sleep(100); //Sleep for 100ms before looping again   
  finished = props.get(&quot;index.finished&quot;);
  time = time + 100;
}

vars.put(&quot;stayinWhile&quot;,&quot;false&quot;)

if (failed == 1)
{
  SampleResult.setSuccessful(false);
  SampleResult.setResponseCode(&quot;500&quot;);
  SampleResult.setResponseMessage(&quot;Reindex timeout waiting for Confluence&quot;);
}
